{
  "manifest_version": 3,
  "name": "Cypher",
  "version": "1.0.0",
  "description": "Summarize webpages & note them with one click",
  "action": { 
    "default_popup": "index.html"
  }, 
  "permissions": [
    "tabs",
    "scripting",
    "storage",
    "downloads"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "32": "bill.jpg"
  }
}