{
  "manifest_version": 3,
  "name": "Cypher",
  "version": "1.0.0",
  "description": "Summarize webpages & note them with one click",
  "action": { 
    "default_icon": {
      "128": "Cypher.png"
    },
    "default_popup": "index.html"
  }, 
  "permissions": [
    "tabs",
    "scripting",
    "storage",
    "downloads"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "16": "Cypher.png",
    "48": "Cypher.png",
    "128": "Cypher.png"
  }
}